{
    "semanticHighlighting" : true,
    "tokenColors" : [
        {
            "scope" : "source",
            "settings" : {
                "foreground" : "#d4d4d4"
            }
        },
        {
            "scope" : ["string", "document.property.value.amly", "object.data.value.amly"],
            "settings" : {
                "foreground" : "#d89845"
            }
        },
        {
            "scope" : ["entity.name.type.class", "entity.name.type"],
            "settings" : {
                "foreground" : "#36c7cc"
            }
        },
        {
            "scope" : ["storage.type", "variable.language", "constant", "keyword"],
            "settings" : {
                "foreground" : "#569cd6"
            }
        },
        {
            "scope" : "constant.numeric",
            "settings" : {
                "foreground" : "#b5cea8"
            }
        },
        {
            "scope" : "object.amly",
            "settings" : {
                "foreground" : "#1fc0ff",
            }
        },
        {
            "scope" : "object.type.amly",
            "settings" : {
                "foreground" : "#fbff00",
            }
        },
        {
            "scope" : "object.data.amly",
            "settings" : {
                "foreground" : "#00d612",
            }
        },
        {
            "scope" : ["object.data.key.amly", "document.property.key.amly"],
            "settings" : {
                "foreground" : "#00b7ff"
            }
        },
        {
            "scope" : ["object.data.assign.amly", "document.property.assign.amly", "document.amly"],
            "settings" : {
                // "foreground" : "#d9ff00"
                "foreground" : "#d4d4d4"
            }
        },
        {
            "scope" : ["block.dict.open.amly", "block.dict.close.amly"],
            "settings" : {
                "foreground" : "#91ff00"
            }
        },
        {
            "scope" : ["block.list.open.amly", "block.list.close.amly"],
            "settings" : {
                "foreground" : "#00ff9d"
            }
        },
        {
            "scope" : "comment",
            "settings" : {
                "foreground" : "#23a323"
            }
        },
        {
            "scope" : "support.type.property-name.json.comments",
            "settings" : {
                "foreground" : "#9cdcfe"
            }
        },
        {
            "scope" : "constant.language.json.comments",
            "settings" : {
                "foreground" : "#19dcff",
            }
        },
        {
            "scope" : ["document.open.amly","document.close.amly"],
            "settings" : {
                "foreground" : "#fffb00",
                "fontStyle" : "bold underline"
            }
        }
    ]
}