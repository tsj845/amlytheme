{
    "tokenColors":[
        {
            "scope": "document.amly",
            "settings": {
                "foreground": "#ff008c"
            }
        },
        {
            "scope": "object.amly",
            "settings": {
                "foreground": "#1fc0ff",
            }
        },
        {
            "scope": "object.type.amly",
            "settings": {
                "foreground": "#fbff00",
            }
        },
        {
            "scope": "object.data.amly",
            "settings": {
                "foreground": "#00d612",
            }
        },
        {
            "scope": ["object.data.key.amly", "document.property.key.amly"],
            "settings": {
                "foreground": "#00e1ff"
            }
        },
        {
            "scope": ["object.data.value.amly", "document.property.value.amly"],
            "settings": {
                "foreground": "#ff7300"
            }
        },
        {
            "scope": ["object.data.assign.amly", "document.property.assign.amly"],
            "settings": {
                "foreground": "#d9ff00"
            }
        },
        {
            "scope": ["block.dict.open.amly", "block.dict.close.amly"],
            "settings": {
                "foreground": "#91ff00"
            }
        },
        {
            "scope": ["block.list.open.amly", "block.list.close.amly"],
            "settings": {
                "foreground": "#00ff9d"
            }
        },
        {
            "scope": "comment.amly",
            "settings": {
                "foreground": "#0f9442"
            }
        },
        {
            "scope": "support.type.property-name.json.comments",
            "settings": {
                "foreground": "#12aad8",
            }
        },
        {
            "scope": "constant.language.json.comments",
            "settings": {
                "foreground": "#19dcff",
            }
        },
        {
            "scope": "number.amly",
            "settings": {
                "foreground": "#0084ff",
            }
        },
        {
            "scope": ["document.open.amly","document.close.amly"],
            "settings": {
                "foreground": "#fffb00",
                "fontStyle": "bold underline"
            }
        }
    ]
}