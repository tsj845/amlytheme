{
    "name": "amlytheme",
    "displayName": "amly colors",
    "description": "syntax highlighting for .amly files",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.63.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "amly",
            "aliases": ["amly", "amly"],
            "extensions": [".amly"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "amly",
            "scopeName": "source.amly",
            "path": "./syntaxes/amly.tmLanguage.json"
        }],
        "themes": [{
            "id": "amly-theme",
            "label": "amly dark",
            "path": "./colorthemes/amlydark-color-theme.json",
            "uiTheme": "vs-dark"
        }]
    }
}